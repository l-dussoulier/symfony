    <body class="container">

    {% include 'nav.html' %}

    	<h1 class="text-primary">Liste Emprunt terminer</h1>

      <table class="table table-striped table-hover">
            <tr>
                <td> Id emprunt </td>
                <td> Date du pret </td>
                <td> Date retour demander </td>
                <td> Date retour effectif </td>

            </tr>
            {% for Tab in listeFinEm %}
            {% if Tab.DateRetourEffectif != NULL %}
      			<tr>
      				<td>{{Tab.idEmprunt}} </td>
              <td>{{Tab.DatePret|date("d/m/Y")}} </td>
              <td>{{Tab.DateRetourDemander|date("d/m/Y")}} </td>
              <td>{{Tab.DateRetourEffectif|date("d/m/Y") }}</td>
            </tr>
          {% endif %}
    		{% endfor %}
    	</table>

