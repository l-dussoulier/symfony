    <body class="container">

    {% include 'nav.html' %}
    <div class="cont">
    	<h1 class="text-primary">Liste Emprunt terminer</h1>
        <table class="table table-striped table-hover">
            <tr>
                <td><b> Id emprunt </b></td>
                <td><b> Date du pret </b></td>
                <td><b> Date retour demander </b></td>
                <td><b> Date retour effectif </b></td>

            </tr>
            {% for Tab in listeFinEm %}
            {% if Tab.DateRetourEffectif != NULL %}
      			<tr>
      				<td>{{Tab.idEmprunt}} </td>
              <td>{{Tab.DatePret|date("d/m/Y")}} </td>
              <td>{{Tab.DateRetourDemander|date("d/m/Y")}} </td>
              <td>{{Tab.DateRetourEffectif|date("d/m/Y") }}</td>
            </tr>
          {% endif %}
    		{% endfor %}
    	</table>
  </div>
